msgstr[0] "aurrez existitzen diren %s iruzkin ezikusi da"
msgstr[1] "aurrez existitzen diren %s iruzkin ezikusi dira"

msgid "Completed GreyMatter import!"
msgstr "GreyMatter inportazioa osatu da!"

